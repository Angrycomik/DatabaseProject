<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bd.InsertArtistController">
<top>
    <fx:include source="Header.fxml" />
</top>
   <center>
      <VBox fx:id="rootVBox" alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="25.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Wprowadź dane artysty:">
               <font>
                  <Font name="Arial" size="36.0" />
               </font>
            </Label>
            <TextField fx:id="artistName" editable="false" promptText="Imie i nazwisko / Nazwa" />
            <TextField fx:id="startCareer" prefWidth="946.0" promptText="Rok rozpoczęcia kariery" />
            <TextField fx:id="endCareer" promptText="Rok zakończenia" />
            <Button fx:id="addBtn" mnemonicParsing="false" onAction="#onAddClick" text="Dodaj" />
         </children>
         <padding>
            <Insets left="400.0" right="400.0" top="-200.0" />
         </padding>
      </VBox>
   </center>
</BorderPane>
